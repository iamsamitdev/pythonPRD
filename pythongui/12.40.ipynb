    "import tkinter as tk\n",
    "mainframe = tk.Tk()\n",
    "mainframe.title(\"Main Frame\")\n",
    "\n",
    "tp = tk.Toplevel()\n",
    "tp.title(\"Toplevel Frame\")\n",
    "lbl = tk.Label(tp, text=\"This is Toplevel widget.\")\n",
    "lbl.pack(padx=50, pady=50)\n",
    "\n",
    "mainframe.mainloop()"
   ]
