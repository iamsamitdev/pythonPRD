   "source": [
    "import tkinter as tk\n",
    "mainframe = tk.Tk()\n",
    "# สร้าง Spinbox widget เริ่มต้นจากตัวเลขที่ 0 จนถึง 20 ทศนิยม 2 ตำแหน่ง\n",
    "spb = tk.Spinbox(mainframe, from_=0, to=20, format=\"%10.2f\")\n",
    "spb.grid(column=0, row=0, padx=30, pady=30)\n",
    "\n",
    "mainframe.mainloop()"
   ]
