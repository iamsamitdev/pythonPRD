   "source": [
    "import tkinter as tk\n",
    "from tkinter import filedialog\n",
    "mainframe = tk.Tk()\n",
    "def openfile():\n",
    "    tk.filedialog.askopenfilename()\n",
    "\n",
    "btn = tk.Button(mainframe, text=\"OpenFile\", \n",
    "                command=openfile, width=10)\n",
    "btn.pack(padx=10, pady=10)\n",
    "\n",
    "mainframe.mainloop()"
   ]
